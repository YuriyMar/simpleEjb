	<queryString>
		<![CDATA[/*Запрос материалы  */
Select
   mat_name ,mu_name , cast(sum(countgen) as decimal) as countgen  ,
                       cast(cost as decimal) ,
                       cast(sum(stoimost) as decimal) as stoimost ,
                       bal_sch  , nn , kindrefcode, accountingtyperefcode
From (
Select distinct
      finmat.mat_name
	, finmat.code
        , finmat.mu_name
        , finmat.quantity  as countgen
        , finmat.calc_price  as cost
        , coalesce(finmat.cost,0) as stoimost
        , finmat.bal_sch
        , finmat.nn
        , estimate.kindrefcode
        , estimate.accountingtyperefcode
  From
         enestimateitem estimate
       , finmaterials finmat
       , enact2enplanwork ena2
       , enplanworkitem pwi
       , tktechcard tkd
       , tkclassificationtype tkcls
where
ena2.actrefcode = $P{PcodeAkt}
  and ena2.plancode = estimate.planrefcode
  and finmat.estimateitemrefcode = estimate.code
  and finmat.statusrefcode = 1
  and estimate.kindrefcode = 1
  and finmat.cost <> 0
  and pwi.planrefcode = ena2.plancode
  and pwi.kartarefcode = tkd.code
  and pwi.code = estimate.planitemrefcode
  and tkd.classificationtypecode = $P{calculationcode}
  /*Если costworkcontractor не пустая, значит работа сторонняя - не включаем в расходный акт*/
  and tkcls.code = tkd.classificationtypecode
  and (tkcls.costworkscontractor is null
  or tkcls.costworkscontractor = 0)

  union all

  Select distinct
       finmat.mat_name||('(ПММ)')  as mat_name
	, finmat.code
        , finmat.mu_name
        , finmat.quantity  as countgen
        , finmat.calc_price  as cost
        , coalesce(finmat.cost,0) as stoimost
        , finmat.bal_sch
        , finmat.nn
        , estimate.kindrefcode
        , estimate.accountingtyperefcode
  From
         enestimateitem estimate
       , finmaterials finmat
       , enact2enplanwork ena2
       , enplanworkitem pwi
       , tktechcard tkd
       , tkclassificationtype tkcls
       , entransportitem transport
       , entransport2enestimate t2e
where
ena2.actrefcode = $P{PcodeAkt}
  and ena2.plancode = estimate.planrefcode
  and finmat.estimateitemrefcode = estimate.code
  and finmat.statusrefcode = 1
  and estimate.kindrefcode = 2
  and finmat.cost <> 0
  and pwi.planrefcode = ena2.plancode
  and pwi.kartarefcode = tkd.code
  and pwi.code = transport.planitemrefcode
  and tkd.classificationtypecode = $P{calculationcode}
  and transport.code = t2e.transportrefcode
  and t2e.estimaterefcode = estimate.code
    /*Если costworkcontractor не пустая, значит работа сторонняя - не включаем в расходный акт*/
  and tkcls.code = tkd.classificationtypecode
  and (tkcls.costworkscontractor is null
  or tkcls.costworkscontractor = 0)

union all

Select distinct
               ss.name as mat_name
        , ss.code
        , 'ШТ' as mu_name
        , 1  as countgen
        , ss.cost  as cost
        , ss.cost as stoimost
        , ss.account as bal_sch
        , ss.buildnumber as nn
        , estimate.kindrefcode
        , estimate.accountingtyperefcode
  From
         enestimateitem estimate
       , scseal ss
       , enact2enplanwork ena2
       , enplanworkitem pwi
       , tktechcard tkd
       , tkclassificationtype tkcls
where
ena2.actrefcode = $P{PcodeAkt}
  and ena2.plancode = estimate.planrefcode
  and ss.estimateitemrefcode = estimate.code
  and ss.statusrefcode in (1,3)
  and estimate.kindrefcode = 1
  and pwi.planrefcode = ena2.plancode
  and pwi.kartarefcode = tkd.code
  and pwi.code = estimate.planitemrefcode
   and tkd.classificationtypecode = $P{calculationcode}
  /*Если costworkcontractor не пустая, значит работа сторонняя - не включаем в расходный акт*/
  and tkcls.code = tkd.classificationtypecode
  and (tkcls.costworkscontractor is null
  or tkcls.costworkscontractor = 0)

union all

  /* Стоимость счетчика для усуг на сторону */
select s.name|| case when (select  count(a.code)
   from enmetrologycounter mc, enplanwork pw,
   enact2enplanwork a2pw, enact a
   where mc.invnumber = s.invnumber
   and pw.elementrefcode = mc.elementcode
   and pw.kindcode = 4
   and a2pw.plancode = pw.code
   and a2pw.actrefcode = a.code
   and a.statusrefcode = 3
   and a.acttyperefcode = 38
   )  > 0 then ' - з урахуванням вартості параметризації'
   else s.name end as mat_name
    , s.code
    , 'лічильник' as mu_name
    , 1 as countgen
    , coalesce(s.costold, 0) as cost
    , coalesce(s.costold, 0) as stoimost
    , s.account as bal_sch
    , s.invnumber as nn
    , es.kindrefcode
    , es.accountingtyperefcode
  from sccounter s, enestimateitem es, enact2enplanwork ena2, enact a
 where a.code = ena2.actrefcode
   and s.estimateitemrefcode = es.code
   and es.countfact <> 0
   and es.kindrefcode = 1
   and ena2.plancode = es.planrefcode
   and ena2.actrefcode = $P{PcodeAkt}


) sel1
 group by sel1.mat_name ,   sel1.mu_name ,
               sel1.bal_sch , sel1.nn,  sel1.cost  ,kindrefcode, accountingtyperefcode
  order by  nn , bal_sch , mat_name , mu_name , cost]]>

	<parameter name="PcodeAkt" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(500000009)]]></defaultValueExpression>
	</parameter>
	<parameter name="PcalcCode" class="java.lang.Double"/>
	<queryString>
		<![CDATA[select
matcode,
nn,
name,
namemeasure,
price,
sum(quantity) as quantity,
round(cast((sum(quantity)*price) as decimal),2) as cost,
fullcost,
finmolcode,
finmolname
from
(
select distinct
	ma.code,
	ma.nn,
	mat.code as matcode,
	me.name as namemeasure,
	mat.name as name,
	ma.price,
	ma.quantity,
	ma.fullcost,
        act.finmolcode,
        act.finmolname

from
((enestimateitem as es
	inner join
	(tkmaterials as mat
		inner join
		tkmeasurement as me
		on
		me.code = mat.measurementcode
	)
	on
	mat.code = es.materialrefcode)
	inner join
	finmaterials as ma
	on
	ma.estimateitemrefcode = es.code)
	inner join
	enact2enplanwork as acpw
	on
	acpw.plancode = es.planrefcode
    left join enplanworkitem pwi on pwi.code = es.planitemrefcode
    left join  tktechcard tk on pwi.kartarefcode = tk.code,  enact act
where
es.kindrefcode = 5
and acpw.actrefcode = $P{PcodeAkt}
and act.code = acpw.actrefcode

) as Produced_materials
group by
	matcode,
	nn,
	name,
	namemeasure,
	price,
        fullcost,
finmolcode,
finmolname]]>
	</queryString>

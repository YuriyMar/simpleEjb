	<parameter name="PcodeAkt" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(500000009)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
matcode,
nn,
name,
namemeasure,
price,
sum(quantity) as quantity,
fullcost
from
(
select distinct
	ma.code,
	ma.nn,
	mat.code as matcode,
	me.name as namemeasure,
	mat.name as name,
	ma.price,
	ma.quantity,
    (select sum(q1.fullcost)
    from (select distinct ma1.rest_purpose_id, ma1.fullcost
    from finmaterials as ma1
    	inner join enestimateitem as es1 on ma1.estimateitemrefcode = es1.code
        inner join enact2enplanwork as acpw1 on es1.planrefcode = acpw1.plancode
    where acpw1.actrefcode = acpw.actrefcode
    	and es1.kindrefcode = 5
        and ma1.statusrefcode = 1
    ) as q1) as fullcost

from
enestimateitem as es
inner join tkmaterials as mat on mat.code = es.materialrefcode
inner join tkmeasurement as me on me.code = mat.measurementcode
inner join finmaterials as ma on ma.estimateitemrefcode = es.code
inner join enact2enplanwork as acpw on acpw.plancode = es.planrefcode
where
es.kindrefcode = 5
and acpw.actrefcode = $P{PcodeAkt}
and ma.statusrefcode = 1) as Produced_materials
group by
	matcode,
	nn,
	name,
	namemeasure,
	price,
	fullcost]]>
	</queryString>

	<queryString>
		<![CDATA[select
matcode,
nn,
name,
namemeasure,
price,
sum(quantity) as quantity,
(sum(quantity)*price) as cost,
finmolcode,
finmolname,
dategen,
dateedit,
numbergen,
bal_sch,
fullcost,
partner,
partner_name
from
(
select distinct
	ma.code,
	ma.nn,
	mat.code as matcode,
	me.name as namemeasure,
	coalesce(ma.mat_name,mat.name) as name,
	ma.price,
	ma.quantity,
	ma.div_name AS finmolname,
	ma.div_code AS finmolcode,
	ac.dategen,
	ac.dateedit,
	ac.numbergen,
	ma.bal_sch,
	ma.fullcost,
	ma.partner,
	ma.partner_name

from
((enestimateitem as es
	inner join
	(tkmaterials as mat
		inner join
		tkmeasurement as me
		on
		me.code = mat.measurementcode
	)
	on
	mat.code = es.materialrefcode)
	inner join
	finmaterials as ma
	on
	ma.estimateitemrefcode = es.code)
	inner join
	(enact2enplanwork as acpw
		inner join
		enact as ac
		on
		ac.code = acpw.actrefcode)
	on
	acpw.plancode = es.planrefcode
where
es.kindrefcode = 4
and ac.code = $P{PcodeAkt}) as Produced_materials
group by
	matcode,
	nn,
	name,
	namemeasure,
	price,
	finmolname,
	finmolcode,
	dategen,
	dateedit,
	numbergen,
	bal_sch,
	fullcost,
	partner,
	partner_name]]>
	</queryString>

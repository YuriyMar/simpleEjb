	<queryString>
		<![CDATA[SELECT worknumber
	, workname
	, max(countgen) AS countgen
	, sum(itogohh) AS itogohh
	, 'ШТ' AS measurement
	, timework
FROM
	(SELECT tc.code
		, cl.kod AS worknumber
		, cl.name AS workname
		, sum(pi.countgen) AS countgen
		, sum(pi.timegen) AS itogohh
		, coalesce((select sum(tc1.normoftime) FROM tktechcard AS tc1 WHERE tc1.classificationtypecode = cl.code), 0) AS timework
		, 'ШТ' AS measurement
	FROM
	tkclassificationtype AS cl
	INNER JOIN tktechcard AS tc ON tc.classificationtypecode = cl.code
	INNER JOIN enplanworkitem AS pi ON tc.code = pi.kartarefcode
	INNER JOIN enplanwork AS pw ON pi.planrefcode = pw.code
	INNER JOIN enact2enplanwork AS acpw ON pw.code = acpw.plancode
	WHERE
	acpw.actrefcode = $P{PcodeAkt}
	AND pi.countgen > 0
	AND coalesce(cl.costworkscontractor, 0) = 0
	GROUP BY cl.code, tc.code) AS q
GROUP BY worknumber, workname, timework
ORDER BY worknumber]]>
	</queryString>

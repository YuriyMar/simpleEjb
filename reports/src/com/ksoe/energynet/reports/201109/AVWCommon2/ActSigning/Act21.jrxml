	<queryString>
		<![CDATA[select numbergen , dategen , dateact , buhname ,  ename ,  invnumber , year , mm  ,
datestart, datefinal, departmentname, etype, invnumber_byt, planworktype,
planworkstate, isafterdate70, info_prisoed,
/* процент общепроизводственных затрат */
 /*case
     when isActDateAfter01102016 = 1 then 50
     when isActDateAfter01052014 +
           isServicesDateAfter01052014 +
           isTechCondDateAfter01052014 +
           isOperativeDateAfter01052014 = 4 then 60
      else 70 end  as productionExpencesPercent */
/* переделал выборку процента общепроизводственных затрат на функцию 16.05.2017 */
(select * from get_productionexpencespercent($P{PcodeAkt}))  as productionExpencesPercent
/* процент общепроизводственных затрат */
,
/*SUPP-17561 наявність перевірки комплексу робіт*/
(select
count(hu1.code) > 0
from
enact as ac1
inner join enact2enplanwork as acpw1 on ac1.code = acpw1.actrefcode
inner join enplanwork as pw1 on acpw1.plancode = pw1.code
inner join enplanworkitem as pi1 on pw1.code = pi1.planrefcode
inner join enhumenitem as hu1 on pi1.code = hu1.planitemrefcode
inner join finworker as fo1 on hu1.finworkercode = fo1.code
where
pw1.typerefcode = 7 /*Приєднання*/
and hu1.countfact > 0
and pi1.countgen > 0
and pi1.kartarefcode in (2017017496
,2017017494
,2017017495)
and ac1.code = $P{PcodeAkt}
) as is_checking_works
,enconnectionkind
, case when $P{PcodeAkt}  = 1018406426 then
'Дог.№ 1092261 від 29.05.2018/Дог. з ФК.№ 67106 від 29.05.2018' ||
'Предмет договору: Будівництво сонячної електростанції потужністю 7 МВт' ||
'в межах Виноградівської сільської ради Олешківського р-ну Херсонської області (3 черга будівництва),' ||
'Дог.№ 1099445 від 16.04.2018/Дог. з ФК.№ 66321 від 16.04.2018' ||
'Предмет договору: Будівництво сонячної електростанції потужністю 7 МВт' ||
'в межах Виноградівської сільської ради Олешківського р-ну Херсонської області (4 черга будівництва)'  else serv_info end as serv_info
 , projectcipher
 , projectname
 , investinfo

 , (select a2d.dfdoccode from enact2dfdoc a2d where a2d.actrefcode = $P{PcodeAkt} order by a2d.code desc limit 1) as doccode

from
(select numbergen , dategen , dateact , buhname ,  ename ,  invnumber , year , mm  ,
 selperiod.datestart , selperiod.datefinal,  selperiod.departmentname, etype,
 selda.invnumber_byt
, planworktype
, planworkstate
/*Если дата акта больше чем 01 октября 2016*/
, case when  dategen >= to_date('01.10.2016','dd.mm.yyyy')  then 1 else 0 end  as isActDateAfter01102016
/* если дата акта больше чем 01,05,2012 значит признак делаем для того что бы можно было накидывать 70 процентов общепроизв*/
, case when  dategen >= to_date('01.05.2012','dd.mm.yyyy')  then 1 else 0 end  as isafterdate70
, coalesce((Select distinct 'Дог.№ '||tcs.fincontractnumber||' від ' ||to_char(tcs.fincontractdate,'dd.mm.yyyy')
|| ' ' || ( select group_concat(ct.contragentname,',') from ENCONTRAGENT ct where ct.techcondservicesrefcod = tcs.code ) as info_prisoed
from ENTECHCOND2PLANWORK tc2pl  , ENTECHCONDITIONSSERVCS tcs
where tc2pl.planrefcode in (
select a2p.plancode from enact2enplanwork a2p
where a2p.actrefcode = $P{PcodeAkt})
and tcs.code = tc2pl.techconservicesrefcode
limit 1 ),' ') as info_prisoed,
/* вычисление процента общепроизводственных затрат */
 case when dategen >= to_date('01.05.2014','dd.MM.yyyy') then 1
  else 0 end  as isActDateAfter01052014,
coalesce((select case when tcs.contractdate  >= to_date('01.05.2014','dd.MM.yyyy') then 1
else 0 end
from ENTECHCOND2PLANWORK tc2pl  , ENTECHCONDITIONSSERVCS tcs
where tc2pl.planrefcode in (
select a2p.plancode from enact2enplanwork a2p
where a2p.actrefcode = $P{PcodeAkt})
and tcs.code = tc2pl.techconservicesrefcode
limit 1
),1) as isTechCondDateAfter01052014,
coalesce((select case when contractdate >= to_date('01.05.2014','dd.MM.yyyy') then 1
else 0 end from enservicesobject
where enservicesobject.elementcode = (select elementcode from enact where code = $P{PcodeAkt})),1)
 as isServicesDateAfter01052014,
coalesce((select case when contractdate >= to_date('01.05.2014','dd.MM.yyyy') then 1
else 0 end from enoperativeobject
where enoperativeobject.elementcode = (select elementcode from enact where code = $P{PcodeAkt})),1)
 as isOperativeDateAfter01052014
/* вычисление процента общепроизводственных затрат */
, (select  ck.name from
        enservicesobject2techcondtnsservices so2tc,
        entechconditionsservcs tc, enconnectionkind ck
        where so2tc.techcondservrefcode = tc.code
        and tc.connectionkindrefcode = ck.code
        and tc.code in ( select t2p.techconservicesrefcode from enact2enplanwork a2p , entechcond2planwork t2p
                         where a2p.actrefcode = selda.actcode
                         and a2p.plancode=t2p.planrefcode) limit 1 ) as enconnectionkind
 , coalesce((Select distinct 'Дог.№ '||so.contractnumberservices ||' від ' ||to_char(so.contractdateservices,'dd.mm.yyyy')
|| '/' || 'Дог. з ФК.№ '||so.contractnumber ||' від ' ||to_char(so.contractdate,'dd.mm.yyyy')  || ' Предмет договору: ' ||  so.commentservicesgen as serv_info
from enservicesobject so  , enservices2plan so2p
where so.code = so2p.servicesobjectrefcode
and so2p.planrefcode in (
select a2p.plancode from enact2enplanwork a2p
where a2p.actrefcode = $P{PcodeAkt})),'')  as serv_info
, (select pp.projectcipher  from enplanproject pp where pp.planrefcode in (
select * from net.get_codemonthplan_byanycodeplan(   (Select  ena2.plancode::integer
										                      from  enact2enplanwork ena2
										                      where ena2.actrefcode = $P{PcodeAkt} limit 1  ) )
order by pp.code limit 1 ) ) as projectcipher
, (select pp.projectname  from enplanproject pp where pp.planrefcode in (
select * from net.get_codemonthplan_byanycodeplan(   (Select  ena2.plancode::integer
										                      from  enact2enplanwork ena2
										                      where ena2.actrefcode = $P{PcodeAkt} limit 1  ) )
order by pp.code limit 1 ) ) as projectname


, (
Select  max(COALESCE(investinfo))  as investinfo  from (
Select  ip.yeargen ||'р. '|| x."name" || ' пункт ' || i.itemnumber  as investinfo
 from
 enipitem2plan i2p, enipitem i , enplanwork pw , eninvestprogramgroups x , enip ip ,
 (
 select (select * from get_codemonthplan_byanycodeplan(a2p.plancode::numeric)) as codemonthplan  , a2p.plancode as codefactplan
 from enplanwork pp , enact2enplanwork a2p, enact a
 where pp.code = a2p.plancode
 and a2p.actrefcode=a.code
 and a2p.actrefcode=$P{PcodeAkt} ) as q1
  where pw.code = q1.codemonthplan
  and i2p.planrefcode = pw.code
  and i2p.ipitemrefcode = i.code
  and i.invgrouprefcode = x.code
  and i.iprefcode = ip.code

  UNION ALL
Select pw.yeargen||'р. '|| x."name" || ' пункт ' || pw.investitemnumber  as investinfo
 from
  enplanwork pw , eninvestprogramgroups x ,
 (
 select (select * from get_codemonthplan_byanycodeplan(a2p.plancode::numeric)) as codemonthplan  , a2p.plancode as codefactplan
 from enplanwork pp , enact2enplanwork a2p, enact a
 where pp.code = a2p.plancode
 and a2p.actrefcode=a.code
 and a2p.actrefcode = $P{PcodeAkt} ) as q1
  where pw.code = q1.codemonthplan
  and pw.invgrouprefcode = x.code
  and pw.code not in ( select ip.planrefcode from enipitem2plan ip where ip.planrefcode = pw.code  )
 ) q7
 ) investinfo


 from (
select ena.numbergen , ena.dategen , ena.dateact,  enda.etype
      , COALESCE( enda.buhname , 'Не заведено' ) as buhname
      ,  enda.ename ,  enda.invnumber
       , to_char(ena.dategen,'yyyy') as year
       , case when to_char(ena.dategen,'mm') = '01' then 'Січень'
              when to_char(ena.dategen,'mm') = '02' then 'Лютий'
              when to_char(ena.dategen,'mm') = '03' then 'Березень'
              when to_char(ena.dategen,'mm') = '04' then 'Квітень'
              when to_char(ena.dategen,'mm') = '05' then 'Травень'
              when to_char(ena.dategen,'mm') = '06' then 'Червень'
              when to_char(ena.dategen,'mm') = '07' then 'Липень'
              when to_char(ena.dategen,'mm') = '08' then 'Серпень'
              when to_char(ena.dategen,'mm') = '09' then 'Вересень'
              when to_char(ena.dategen,'mm') = '10' then 'Жовтень'
              when to_char(ena.dategen,'mm') = '11' then 'Листопад'
              when to_char(ena.dategen,'mm') = '12' then 'Грудень'
            end  as mm,
		ena.invnumber as invnumber_byt
, (select  p.typerefcode from enact2enplanwork a2p  , enplanwork p where a2p.plancode = p.code and a2p.actrefcode = ena.code  limit 1   ) as planworktype
, (select  p.staterefcode from enact2enplanwork a2p  , enplanwork p where a2p.plancode = p.code and a2p.actrefcode = ena.code  limit 1   ) as planworkstate
 , ena.elementcode   , ena.code as actcode   from enact ena  , enelement ene ,  enelementdata enda

where ena.elementcode = ene.code
  and enda.ecode = ene.code
  and ena.code = $P{PcodeAkt}
                 ) selda ,
   				    (
                      Select  min(enworkorder.dategen ) as datestart , max(enworkorder.dategen ) as datefinal, max(endepartment.code) as depcode
                      , max(endepartment.shortname) as departmentname from
                      enworkorder , enworkorder2enplanwork , enplanwork , endepartment
                      where enworkorder.code = enworkorder2enplanwork.workordercode
                      and enplanwork.code = enworkorder2enplanwork.plancode
			and endepartment.code = enplanwork.departmentrefcode
                      and enplanwork.code in
                     (  Select  enp.code
                      from enact ena , enact2enplanwork ena2 , enplanwork enp
                      where ena.code = $P{PcodeAkt}
                      and ena.code = ena2.actrefcode
                      and ena2.plancode = enp.code )
                      ) selperiod
limit 1
) as qwe]]>
	</queryString>

	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["C:\\iReport-3.0.0\\tmp\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="PcodeAkt" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(500018882)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[/* Запрос материалы */
Select
	mat_name
	,wear_date
	,nn
	,mu_name
	,sum(countgen) as countgen
	,cost
	,sum(stoimost) as stoimost
	,bal_sch
	,kindrefcode
	,actnumber
	,actdate
	,div_code
From (
	Select
		Case
			When estimate.kindrefcode = 2
			Then finmat.mat_name||('(ПММ)')
		Else
			finmat.mat_name end as mat_name
		,finmat.mu_name
		,finmat.quantity  as countgen
		,finmat.calc_price  as cost
		,coalesce(finmat.cost,0) as stoimost
		,' '||finmat.bal_sch as bal_sch
		,finmat.nn
		,estimate.kindrefcode
		, to_char(finmat.wear_date,'dd.mm.yyyy') as wear_date
		,a.numbergen as actnumber
		,to_char(a.dategen,'dd.mm.yyyy') as actdate
		,' '||finmat.div_code as div_code
	From
		enestimateitem estimate
		,finmaterials finmat
		,enact2enplanwork ena2
		,enact a
	where
		ena2.actrefcode = $P{PcodeAkt}
		and ena2.plancode = estimate.planrefcode
		and finmat.estimateitemrefcode = estimate.code
		and finmat.statusrefcode = 1
		and estimate.kindrefcode in (3)
		and finmat.cost<> 0
		and ena2.actrefcode = a.code
) sel1
group by
	sel1.mat_name
	,sel1.mu_name
	,sel1.bal_sch
	,sel1.nn
	,sel1.cost
	,kindrefcode
	,wear_date
	,actnumber
	,actdate
	,div_code
order by
	nn
	,bal_sch
	,mat_name
	,mu_name
	,cost]]>
	</queryString>

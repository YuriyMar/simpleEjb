	</parameter>
	<parameter name="Pperiod" class="java.lang.String">
		<defaultValueExpression><![CDATA[" Виконано робіт"]]></defaultValueExpression>
	</parameter>
	<parameter name="calculationcode" class="java.lang.Double">

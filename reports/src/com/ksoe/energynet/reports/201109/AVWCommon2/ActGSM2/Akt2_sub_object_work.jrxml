	<parameter name="PcodeAkt" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(500000009)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select objworkname , objworkmeasure , objworkcount from (
/*транспорт механизмы*/
Select tr.name as objworkname
      ,'ШТ.' as objworkmeasure
      , count(tr.name) as objworkcount
      , 1 as prizorder from
entransportitem ti , tktransport tr
where ti.planrefcode in ( select a2p.plancode from enact2enplanwork a2p where a2p.actrefcode = $P{PcodeAkt} )
and ti.transportrealcode is null
and ti.transportcode = tr.code
group by tr.name
UNION ALL
/*должностя*/
Select te.name as objworkname
      ,'чол.' as objworkmeasure
      , count(tp.name) as objworkcount
      , 2 as prizorder from
enhumenitem hi , tkposition tp , tkelement te
Where hi.planrefcode in ( select a2p.plancode from enact2enplanwork a2p where a2p.actrefcode = $P{PcodeAkt}  )
And hi.finworkercode is null
And hi.positiongencode = tp.code
And hi.typerefcode in (3,4)  /*только строки введенные вручную(что б откинуть водителей которые автоматом вкинулись при добавлении работы )*/
And te.code = tp.elementcode
And te.elementtypecode = 240000001
Group by te.name
UNION ALL
/*материалы*/
Select tm.name as objworkname
      ,tmm.name as objworkmeasure
      , sum(i.countfact) as objworkcount
      , 3 as prizorder from
enestimateitem i , tkmaterials tm , tkmeasurement tmm
Where i.planrefcode in ( Select a2p.plancode from enact2enplanwork a2p where a2p.actrefcode = $P{PcodeAkt})
And i.materialrefcode = tm.code
And tm.measurementcode = tmm.code
And i.typerefcode in (3,4)
And i.kindrefcode <> 2 /*не равно ПММ*/
Group by tm.name , tmm.name
) s1
Order by s1.prizorder]]>
	</queryString>

	<queryString language="SQL">
		<![CDATA[select 
    ma.div_code as finmolcode,
    ma.div_name as finmolname
from
enestimateitem as es,
tkmaterials as mat,
tkmeasurement as me,
finmaterials as ma,
enact2enplanwork as acpw, 
enact as ac
where
es.kindrefcode = 5
and ac.code = $P{PcodeAkt}
and me.code = mat.measurementcode
and mat.code = es.materialrefcode
and ma.estimateitemrefcode = es.code
and ac.code = acpw.actrefcode
and acpw.plancode = es.planrefcode
group by ma.div_code, ma.div_name]]>
	</queryString>

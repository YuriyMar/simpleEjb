	</variable>
	<variable name="kodzatrat" class="java.lang.String">
		<variableExpression><![CDATA[$F{invnumber} != null ? (
            (( $P{actType}.intValue() == 1 )
	  || ( $P{actType}.intValue() == 2 )
	  || ( $P{actType}.intValue() == 3 )
	  || ( $P{actType}.intValue() == 19 )
	  || ( $P{actType}.intValue() == 4 ) ) ?
((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getKodZatrat($F{invnumber})
: " " )
: " "]]></variableExpression>
		<initialValueExpression><![CDATA[new String("")]]></initialValueExpression>
	</variable>
	<variable name="signnalog" class="java.lang.String">
		<variableExpression><![CDATA[$F{invnumber} != null ? (
            (( $P{actType}.intValue() == 1 )
	  || ( $P{actType}.intValue() == 2 )
	  || ( $P{actType}.intValue() == 3 )
	  || ( $P{actType}.intValue() == 19 )
	  || ( $P{actType}.intValue() == 4 ) ) ?
$F{invnumber} != null ? (
            (( $P{actType}.intValue() == 1 )
	  || ( $P{actType}.intValue() == 2 )
	  || ( $P{actType}.intValue() == 3 )
	  || ( $P{actType}.intValue() == 19 )
	  || ( $P{actType}.intValue() == 4 ) ) ?
/*NET-2665 Если код источника прихода основного = 20, 21, 22 и код затрат содержит 232, 91, 92, то
вместо налог/неналог выводится "безвозмездно полученные" */
( (((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getKodIst($F{invnumber}).equals("20")
|| ((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getKodIst($F{invnumber}).equals("21")
|| ((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getKodIst($F{invnumber}).equals("22"))
&& (
((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getKodZatrat($F{invnumber}).substring(0,3).equals("232")
|| ((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getKodZatrat($F{invnumber}).substring(0,2).equals("91")
|| ((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getKodZatrat($F{invnumber}).substring(0,2).equals("92")
) ?
"безвозмездно полученные"
: ((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getSignNalog($F{invnumber}) )
: " " )
: " "
: " " )
: " "]]></variableExpression>

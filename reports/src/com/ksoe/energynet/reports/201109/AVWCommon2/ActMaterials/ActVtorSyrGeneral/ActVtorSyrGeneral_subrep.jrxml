	<queryString>
		<![CDATA[select
ma.nn
, ma.mat_name
, ma.mat_id
, ma.code
, ma.mu_name
, cast(ma.quantity as decimal) as quantity
, cast(ma.price as decimal) as price
, cast(ma.cost as decimal) as cost
from enact as ac
inner join enact2enplanwork as acpw on acpw.actrefcode = ac.code
inner join enplanwork as pw on acpw.plancode = pw.code
inner join enestimateitem as es on pw.code = es.planrefcode
inner join finmaterials as ma on es.code = ma.estimateitemrefcode
inner join tkmaterials as mat on es.materialrefcode = mat.code
where
ma.statusrefcode = 1
and ma.parentrefcode = $P{finmaterialsCode}
and ac.code = $P{actCode}
and es.kindrefcode = 3]]>
	</queryString>

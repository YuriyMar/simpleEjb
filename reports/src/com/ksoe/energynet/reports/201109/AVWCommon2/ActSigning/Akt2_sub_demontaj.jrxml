	</parameter>
	<queryString>
		<![CDATA[select
  name_material
  ,mu_name
   ,sum(countgen) as countgen
        , nn
        , bal_sch
        , calc_price
        , sum(cost)::numeric(15,2) as cost
from (

Select
         coalesce(fin.mat_name,tkm2.name) as name_material
        , tmes.name as mu_name
        , sum(estimate.countfact) as countgen
        , fin.nn
        , fin.bal_sch
        , fin.calc_price
        , fin.cost
        , fin.quantity
        , estimate.code
  From
         enestimateitem estimate left join finmaterials fin on (estimate.code = fin.estimateitemrefcode )  ,
         tkmaterials tkm2 ,
         tkmeasurement tmes,
         enact2enplanwork a2p
where
a2p.actrefcode = $P{PcodeAkt}
  and  a2p.plancode = estimate.planrefcode
  and estimate.kindrefcode = 3 -- только ДЕМОНТИРОВАННЫЕ  материалы
  and tkm2.code = estimate.materialrefcode
  and tkm2.measurementcode = tmes.code
  and estimate.countfact > 0
  and estimate.accountingtyperefcode not in (5,6,7)
  group by tkm2.name, fin.mat_name, tmes.name , fin.nn  , fin.bal_sch  ,  fin.calc_price , fin.cost  ,fin.quantity  ,   estimate.code
  ) s1
  group by  name_material ,mu_name  , nn  , bal_sch  , calc_price
  order by name_material]]>
	</queryString>
